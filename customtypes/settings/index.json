      "contribution_payment_link": {
        "type": "Link",
        "config": {
          "label": "Contribution Payment Link",
          "placeholder": "",
          "select": null
        }
      },
      "exam_payment_link": {
        "type": "Link",
        "config": {
          "label": "Exam Payment Link",
          "placeholder": "",
          "select": null
